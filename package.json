{
  "name": "embedded-workflow-builder-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "check": "npm run check-spelling && npm run check-markdown && npm run check-typescript",
    "check-typescript": "prettier --log-level warn --check \"src/**/*.ts\"",
    "check-spelling": "cspell --config cspell.jsonc \"docs/**/*.md{x,}\"",
    "check-markdown": "prettier --log-level warn --check \"docs/**/*.md\"",
    "format": "npm run format-markdown && npm run format-typescript",
    "format-markdown": "prettier --log-level warn --write \"docs/**/*.md\"",
    "format-typescript": "prettier --log-level warn --write \"src/**/*.ts\"",
    "generate-public-connector-docs": "PRISMATIC_API_KEY=$(prism me:token) docusaurus generate-connector-docs --public-connectors",
    "postgenerate-public-connector-docs": "npm run format-markdown",
    "generate-public-connector-docs-from-manifest": "PRISMATIC_API_KEY=$(prism me:token) docusaurus generate-connector-docs --public-connectors --from-manifest",
    "postgenerate-public-connector-docs-from-manifest": "npm run format-markdown",
    "generate-private-connector-docs": "PRISMATIC_API_KEY=$(prism me:token) docusaurus generate-connector-docs --private-connector",
    "postgenerate-private-connector-docs": "npm run format-markdown",
    "resize-screenshots": "docusaurus resize-screenshots",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.0",
    "@docusaurus/preset-classic": "3.8.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "lodash.merge": "^4.6.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.8.0",
    "@docusaurus/module-type-aliases": "3.8.0",
    "@docusaurus/tsconfig": "3.8.0",
    "@docusaurus/types": "3.8.0",
    "change-case": "^5.4.4",
    "cli-progress": "^3.12.0",
    "cspell": "^9.0.2",
    "graffle": "^8.0.0-next.161",
    "graphql": "^16.10.0",
    "handlebars": "^4.7.8",
    "jimp": "^1.6.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
